	</parameter>
	<queryString>
		<![CDATA[select pi.patient_identification as pid, pi.first_name as pifn, pi.middle_initial as pimi, pi.last_name as piln,
			pi.birthdate as pibd, pi.gender as pigd, pi.allergies as pial, dt.patient_program as dtpg,
			 pi.diagnosis_code_1 as pidc1, pi.diagnosis_code_2 as pidc2, dt.pharmacy as dtph, ml.medication as mlme, 
			 ml.dosage as mldo, ml.medication_start_date as mlmsd, ml.instructions as mlin, ml.medication_end_date as mlmed, 
			 ml.current_medication as mlcm  from patient_medication_log ml 
              INNER JOIN patient_info pi ON ml.record_ = pi.record_
              INNER JOIN patient_details dt ON dt.record_ = pi.record_
              WHERE pi.record_ = $P{record}]]>
	</queryString>
